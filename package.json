{
  "name": "typescript-test-extension-2",
  "displayName": "typescript-test-extension-2",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "C# Debug Visualizer",
        "properties": {
          "charpDebugVisualizer.colorTheme": {
            "type": "string",
            "description": "Choose color theme according to your preference.",
            "default": "Oceanic Breeze",
            "enum": ["Oceanic Breeze", "Carbon Mist", "Dreamscapes", "Rosy Blush", "Spring Serenity", "Sunburst Glow"]
          }
        }
      }
    ],
    "commands": [
      {
        "command": "typescript-test-extension-2.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
